{
  "build": {
    "env": {
      "HUGO_VERSION": "0.88.1"
    }
  },
  "redirects": [
    { "source": "/branding", "destination": "/brand", "permanent": false },
    { "source": "/branding/", "destination": "/brand", "permanent": false },
    {
      "source": "/act/grant",
      "destination": "/grant/apply",
      "permanent": false
    },
    {
      "source": "/act/research",
      "destination": "/research/contribute",
      "permanent": false
    },
    {
      "source": "/programs/advising",
      "destination": "/archived/advising",
      "permanent": false
    },
    {
      "source": "/advising",
      "destination": "/archived/advising",
      "permanent": false
    },
    {
      "source": "/programs/(.*)",
      "destination": "/$1",
      "permanent": false
    },
    {
      "source": "/research-privacy",
      "destination": "/research/privacy",
      "permanent": false
    },
    {
      "source": "/blog/(.*)",
      "destination": "/journals/$1",
      "permanent": false
    }
  ]
}
