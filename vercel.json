{
  "build": {
    "env": {
      "HUGO_VERSION": "0.78.2"
    }
  },
  "redirects": [
    {
      "source": "/grant",
      "destination": "/programs/grant",
      "permanent": false
    },
    { "source": "/branding", "destination": "/brand", "permanent": false },
    { "source": "/branding/", "destination": "/brand", "permanent": false },
    {
      "source": "/research",
      "destination": "/programs/research",
      "permanent": false
    },
    {
      "source": "/events",
      "destination": "/programs/events",
      "permanent": false
    },
    {
      "source": "/act/grant",
      "destination": "/programs/grant/apply",
      "permanent": false
    },
    {
      "source": "/act/research",
      "destination": "/programs/research/contribute",
      "permanent": false
    },
    {
      "source": "/programs/advising",
      "destination": "/programs/archived/advising",
      "permanent": false
    },
    {
      "source": "/advising",
      "destination": "/programs/archived/advising",
      "permanent": false
    },
    {
      "source": "/blog/(.*)",
      "destination": "/journals/$1",
      "permanent": false
    }
  ]
}
